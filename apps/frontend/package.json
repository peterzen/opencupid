    "vue-circle-flags": "^1.0.9",
    "vue-draggable-next": "^2.2.1",
    "vue-i18n": "^11.1.3",
    "vue-multiselect": "github:peterzen/vue-multiselect",
    "vue-router": "^4.5.0",
    "vue-toastification": "2.0.0-rc.5",
