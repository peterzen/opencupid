    "@vueuse/core": "^13.5.0",
    "@vueuse/integrations": "^13.5.0",
    "altcha": "^2.0.5",
    "animate.css": "^4.1.1",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "bootstrap-vue-next": "^0.30.4",
    "find-up": "^7.0.0",
    "flag-icons": "^7.5.0",
    "i18n-iso-countries": "^7.14.0",
    "luxon": "^3.6.1",
