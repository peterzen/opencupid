{
  "name": "backend",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsup --watch --onSuccess \"node --inspect dist/main.js\"",
    "build": "tsup && node scripts/generate-version.js && tsup --config tsup.scripts.config.ts",
    "start": "node dist/main.js",
    "prisma:generate": "prisma generate --no-hints",
    "prisma:migrate": "prisma migrate --no-hints dev",
    "prisma:seed": "prisma db seed",
    "prisma:deploy": "prisma migrate deploy",
    "watch": "tsc-watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write src/",
    "test-data": "npx tsx src/test-data/Profile.ts",
    "images:reprocess": "npx tsx scripts/reprocess-images.ts",
    "redis:flush": "docker compose exec redis redis-cli FLUSHALL",
    "redis:sessions": "docker compose exec redis  redis-cli KEYS '*'",
    "tags:translate": "npx node scripts/translate-tags-deepl.js",
    "db:psql": "docker compose exec db psql -U appuser app",
    "db:restore": "docker compose exec -T db psql -U appuser app",
    "db:dump": "docker compose exec db pg_dump  --data-only --no-privileges  --disable-triggers -U appuser app",
    "db:backup": "docker compose exec db pg_dump --clean -U appuser app",
    "test": "vitest",
    "test:integration": "vitest --config vitest.integration.config.ts --mode test"
  },
  "prisma": {},
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.1.1",
    "@fastify/websocket": "^11.1.0",
    "@google-cloud/translate": "^9.1.0",
    "@maxmind/geoip2-node": "^6.1.0",
    "@prisma/client": "^6.8.2",
    "@sentry/node": "^9.39.0",
    "@tensorflow-models/blazeface": "^0.1.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "altcha-lib": "^1.2.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.52.2",
    "canvas": "^3.1.2",
    "cuid": "^3.0.0",
    "deepl-node": "^1.19.0",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "fastify": "^5.3.3",
    "fastify-plugin": "^4.5.1",
    "find-up": "^7.0.0",
    "i18next": "^25.2.1",
    "i18next-fs-backend": "^2.6.0",
    "ioredis": "^5.6.1",
    "jsdom": "^26.1.0",
    "node": "^22.19.0",
    "nodemailer": "^7.0.3",
    "otp-generator": "^4.0.1",
    "sharp": "^0.34.2",
    "slugify": "^1.6.6",
    "smartcrop": "^2.0.5",
    "smartcrop-sharp": "^2.0.8",
    "ts-node": "^10.9.2",
    "web-push": "^3.6.7",
    "ws": "^8.18.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.8.0",
    "@types/all-the-cities": "^3.1.3",
    "@types/bcrypt": "^5.0.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.16.1",
    "@types/nodemailer": "^6.4.17",
    "@types/otp-generator": "^4.0.2",
    "@types/web-push": "^3.6.4",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "all-the-cities": "^3.1.0",
    "axios": "^1.9.0",
    "eslint": "^9.26.0",
    "globals": "^16.2.0",
    "mime-types": "^3.0.1",
    "nyc": "^17.1.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.8.2",
    "prisma-zod-generator": "^0.8.13",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.4.0",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^3.2.4",
    "wscat": "^6.1.0",
    "zod-prisma-types": "^3.2.4"
  }
}
